#地址重写
[URL Rewrite]
enable = false
# > TikTok Unlock (By Choler)
(?<=_region=)CN(?=&) 307 US
(?<=&mcc_mnc=)4 307 2
^(https?:\/\/(tnc|dm)[\w-]+\.\w+\.com\/.+)(\?)(.+) 302 $1$3
(^https?:\/\/*\.\w{4}okv.com\/.+&.+)(\d{2}\.3\.\d)(.+) 302 $118.0$3
# > 抖音 去广告&水印需配合脚本使用
^https?:\/\/[\w-]+\.amemv\.com\/aweme\/v\d\/feed\/ header https://aweme.snssdk.com/aweme/v1/feed/
^https?:\/\/[\w-]+\.amemv\.com\/aweme\/v\d\/aweme\/post\/ header https://aweme.snssdk.com/aweme/v1/aweme/post/
^https?:\/\/[\w-]+\.amemv\.com\/aweme\/v\d\/follow\/feed\/ header https://aweme.snssdk.com/aweme/v1/follow/feed/
^https?:\/\/[\w-]+\.amemv\.com\/aweme\/v\d\/nearby\/feed\/ header https://aweme.snssdk.com/aweme/v1/nearby/feed/
^https?:\/\/[\w-]+\.amemv\.com\/aweme\/v\d\/search\/item\/ header https://aweme.snssdk.com/aweme/v1/search/item/
^https?:\/\/[\w-]+\.amemv\.com\/aweme\/v\d\/general\/search\/single\/ header https://aweme.snssdk.com/aweme/v1/general/search/single/
^https?:\/\/[\w-]+\.amemv\.com\/aweme\/v\d\/hot/search\/video\/list\/ header https://aweme.snssdk.com/aweme/v1/hot/search/video/list/
enable = true

